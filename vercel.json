{
    "version": 2,
    "builds": [
      {
        "src": "frontend/index.html",
        "use": "@vercel/react"
      },
      {
        "src": "backend/server.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/backend/$1"
      }
    ]
  }
  